---
layout: layouts/base.njk
---

<div class="home-grid">

{% for roll in site.photos.rolls %}
  {% for item in roll.grid %}
    <a 
      href="/rolls/{{ roll.name }}/"
      class="grid-item{% if item.isTrigger %} trigger{% endif %}"
    >
      <img src="{{ item.src }}" alt="">
    </a>
  {% endfor %}
{% endfor %}

</div>
