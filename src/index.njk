<section class="home-grid">
  {% for rollName in photos.rollsList %}
    {% set roll = photos.rolls[rollName] %}

    {% for img in roll.grid %}
      {% if img == roll.trigger %}
        <a href="/rolls/{{ roll.name }}/">
          <img class="trigger" src="{{ img | url }}" loading="lazy">
        </a>
      {% else %}
        <img src="{{ img | url }}" loading="lazy">
      {% endif %}
    {% endfor %}
  {% endfor %}
</section>
