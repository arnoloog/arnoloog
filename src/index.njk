---
title: ARNOLOOG
layout: layouts/base.njk
---

<section class="grid">

  {% for rollName in photos.rollsList %}
    {% set roll = photos.rolls[rollName] %}

    {% for img in roll.grid %}
      {% set isTrigger = "trigger" in img %}

      <a 
        href="/rolls/{{ rollName }}/" 
        class="{% if isTrigger %}trigger{% endif %}"
      >
        <img src="{{ img | url }}" loading="lazy" alt="">
      </a>

    {% endfor %}
  {% endfor %}

</section>
